# qjackctl.pri
#
PREFIX  = @ac_prefix@
BINDIR  = @ac_bindir@
LIBDIR  = @ac_libdir@
DATADIR = @ac_datadir@
MANDIR  = @ac_mandir@

VERSION = @ac_version@
CONFIG += @ac_build_type@

INCLUDEPATH += @ac_incpath@
LIBS += @ac_libs@

# Extra optimization flags
QMAKE_CXXFLAGS += @ac_cflags@
QMAKE_LFLAGS += @ac_ldflags@

# D-BUS support
QDBUS = @ac_qdbus@
!isEmpty(QDBUS) {
	QT += dbus
}

# Network support
QNETWORK = @ac_qnetwork@
!isEmpty(QNETWORK) {
	QT += network
}
